{
  "extends": [
    "plugin:vue/essential",
    "airbnb-base"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "jquery": true
  },
  "globals": {
    "Vue": true,
    "$": true,
    "jQuery": true,
    "axios": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-console": "off"
  }
}